[run]
source = app
omit = 
    */tests/*
    */test_*.py
    */__pycache__/*
    */migrations/*
    */venv/*
    */env/*
    */site-packages/*
    */dist/*
    */build/*
    app/main.py
    app/core/config.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @property
    @abstractproperty
    class .*\bProtocol\):
    def .*\(self, \.\.\.\):
    pass

precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov

